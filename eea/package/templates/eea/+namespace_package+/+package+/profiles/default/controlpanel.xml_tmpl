<?xml version="1.0"?>
<object name="portal_controlpanel">

  <configlet
    title="${title} Settings"
    action_id="${project}.settings"
    icon_expr="string:portal_url/++resource++${project}.png"
    appId="${project}"
    category="Products"
    condition_expr=""
    url_expr="string:portal_url/@@$package-controlpanel"
    visible="True">
      <permission>Manage portal</permission>
  </configlet>

</object>
